{
    "version": 2,
    "builds": [
      {
        "src": "backend/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "web-dashboard/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/web-dashboard/$1"
      }
    ]
  }
  