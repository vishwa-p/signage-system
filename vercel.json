{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "web-dashboard/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/web-dashboard/$1"
      }
    ]
  }
  